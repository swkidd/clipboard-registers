{
    "manifest_version": 2,
    "name": "Clipboard Registers",
    "description": "Adds clipboard registers to your browser",
    "developer": {
        "name": "swkidd",
        "url": "https://github.com/swkidd"
    },
    "version": "1.0",
    "permissions": [
        "clipboardRead",
        "clipboardWrite",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "clipboard-registers.js"
            ]
        }
    ]
}